<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rdb.refresh.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ViewActivity" />
        <activity android:name=".activity.ScrollActivity" />
        <activity android:name=".activity.ListActivity" />
        <activity android:name=".activity.ListPageActivity" />
        <activity android:name=".activity.GridActivity" />
        <activity android:name=".activity.GridPageActivity" />
        <activity android:name=".activity.RecyclerActivity" />
        <activity android:name=".activity.PageActivity" />
        <activity android:name=".activity.ExpandableListActivity" />
        <activity android:name=".activity.ExpandableListPageActivity" />
    </application>

</manifest>